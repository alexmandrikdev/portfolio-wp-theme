{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"version": 3,
	"settings": {
		"layout": {
			"contentSize": "1280px"
		},
		"spacing": {
			"padding": true,
			"units": [ "px", "em", "rem", "%" ]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "'Inter', sans-serif",
					"name": "Inter",
					"slug": "inter",
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [ "file:./assets/fonts/inter.ttf" ]
						},
						{
							"fontFamily": "Inter",
							"fontStyle": "italic",
							"fontWeight": "100 900",
							"src": [ "file:./assets/fonts/inter-italic.ttf" ]
						}
					]
				}
			]
		}
	}
}
